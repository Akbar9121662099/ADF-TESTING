{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "akbarmm9369"
		},
		"azuresqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuresqldb'"
		},
		"linkedServicedatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedServicedatalake'"
		},
		"ls_AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage1'"
		},
		"ls_databaricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databaricks'"
		},
		"lss_AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lss_AzureBlobStorage1'"
		},
		"linkedServicedatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakemmakbar.dfs.core.windows.net/"
		},
		"ls_AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakemmakbar.dfs.core.windows.net/"
		},
		"ls_HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourcebaseurl}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "akbarmm9369_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azuresqldb_connectionString": {
						"value": "[parameters('azuresqldb_connectionString')]"
					},
					"linkedServicedatalake_accountKey": {
						"value": "[parameters('linkedServicedatalake_accountKey')]"
					},
					"ls_AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage1_accountKey')]"
					},
					"ls_databaricks_accessToken": {
						"value": "[parameters('ls_databaricks_accessToken')]"
					},
					"lss_AzureBlobStorage1_connectionString": {
						"value": "[parameters('lss_AzureBlobStorage1_connectionString')]"
					},
					"linkedServicedatalake_properties_typeProperties_url": {
						"value": "[parameters('linkedServicedatalake_properties_typeProperties_url')]"
					},
					"ls_AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('ls_HttpServer1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "akbarmm9369_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'akbarmm9369_ArmTemplate_0')]"
			]
		},
		{
			"name": "akbarmm9369_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'akbarmm9369_ArmTemplate_1')]"
			]
		}
	]
}